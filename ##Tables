##Tables

/*
create a table

--> datatypes 
// INT( max 4294967295)
// VarChar (max 255)
*/ 


show Databases; // will show the tables
show tables; // will show the tables



create table Cats
(
Name varchar(100),
Breed varchar(100),
age int
);



desc tables; // will give all the columns in table 
show colums from table // will show all the columns as well



drop table tablename; // will drop tables


//pastries databse

create table pastries(
Name varchar(100),
quantity int);



